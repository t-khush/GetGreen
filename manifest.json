{
    "manifest_version": 2,
    "name": "GetGreen",
    "description": "Price Target and Stock Recommendations",
    "version": "1.0",
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["popup.js"]
        }
      ], 
      "browser_action": {
        "default_popup": "popup.html"
    }, 
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "storage", 
        "activeTab",
        "tabs",
        "*://*.robinhood.com/*",
        "webNavigation"
    ]
}